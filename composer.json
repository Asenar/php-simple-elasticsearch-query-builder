{
    "name": "jclaveau/php-simple-elasticsearch-query-builder",
    "description": "A little class to generate ElasticSearch queries in an SQL way",
    "license": "MIT",
    "keywords": ["elasticsearch", "sql", "simple", "query builder"],
    "authors": [
        {
            "name": "Jean Claveau",
            "role": "Developer"
        }
    ],
    "autoload": {
        "psr-4" : {
            "JClaveau\\ElasticSearch\\" : "src"
        },
        "files": [
              "src/functions.php"
        ]
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/jclaveau/php-visibility-violator",
            "no-api": true
        },
        {
            "type": "vcs",
            "url": "https://github.com/webD97/phpdoc-md",
            "no-api": true
        }
    ],
    "require-dev": {
        "phpunit/phpunit": "5.*",
        "webD97/phpdoc-md": "~0.2.0",
        "phpdocumentor/phpdocumentor": "~2.8.0"
    },
    "require": {
        "jclaveau/php-visibility-violator": "dev-master",
        "php": ">=5.6.0"
    }
}
